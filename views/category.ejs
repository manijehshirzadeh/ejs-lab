<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title><%= categoryName %></title>
  </head>
  <body>
    <%- include('./partials/nav') %>
    <ul>
      <% menuItems.forEach((item, index) => {%>
      <li>
        <%= item.name %>, Price: <%= item.price %>, Rating: <%= item.rating
        %>,<%= item.details %>
      </li>
      <% }); %>
    </ul>
  </body>
</html>
